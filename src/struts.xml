<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="logistics" extends="struts-default">
		<action name="LoginAction" class="springLoginAction">
			<result name="success">index.jsp</result>
		</action>
		<action name="RegisterAction" class="springRegisterAction">
			<result name="success">success.jsp</result>
		</action>
		<action name="ManageOrderAction" class="springManageOrderAction">
			<result name="success">JSP/myOrder.jsp</result>
		</action>
	</package>
</struts>    
